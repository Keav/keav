
# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /coffee/

RewriteCond %{HTTP_HOST} !.*keav\.net$ [NC]
RewriteRule ^(.*)$ http://keav.net/coffee/$1 [R=301,L]

RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /coffee/index.php [L]


</IfModule>

# END WordPress
